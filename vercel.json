{
    "version": 2,
    "builds": [
      {
        "src": "PortFolio_BackEnd/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "PortFolio_FrontEnd/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/PortFolio_BackEnd/server.js" },
      { "src": "/(.*)", "dest": "/PortFolio_FrontEnd/$1" }
    ]
  }
  